{
  "name": "@sk-web-gui/web-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn generate:middleware-envs",
    "dev": "cross-env TEST=true next dev",
    "prebuild": "yarn generate:middleware-envs",
    "build": "next build",
    "prebuild:test": "yarn generate:middleware-envs",
    "build:test": "cross-env TEST=true next build",
    "start": "next start",
    "generate:middleware-envs": "node middleware-envs-generator.mjs",
    "generate:contracts": "cross-env NODE_ENV=development npx tsx ./src/swagger-typescript-api.ts",
    "lint": "next lint --no-cache",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "cypress": "dotenv -e .env -- cypress open",
    "cypress:headless": "cross-env TEST=true npx cypress run --component && npx cypress run --e2e",
    "jest": "jest --watch",
    "jest:coverage": "jest --coverage",
    "test": "concurrently \"yarn jest\" \"yarn cypress\"",
    "coverage": "yarn nyc report --reporter=text-summary",
    "coverage:merge": "istanbul-merge --out coverage/summary/coverage-final.json coverage/cypress/coverage-final.json coverage/jest/coverage-final.json && nyc report --reporter=lcov -t coverage/summary --report-dir coverage/summary --check-coverage false",
    "test:coverage": "yarn cypress:headless && yarn jest:coverage && yarn coverage:merge",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@next/bundle-analyzer": "^15.2.4",
    "@sk-web-gui/core": "4.1.1",
    "@sk-web-gui/lucide-icon": "1.1.3",
    "@sk-web-gui/react": "3.0.4",
    "@tailwindcss/forms": "^0.5.10",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "i18next": "^24.2.3",
    "i18next-resources-to-backend": "^1.2.1",
    "lodash": "4.17.21",
    "next": "15.4.8",
    "next-i18n-router": "^5.5.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^15.4.1",
    "sass": "^1.94.0",
    "sharp": "^0.34.5",
    "yup": "^1.7.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@cypress/code-coverage": "^3.14.7",
    "@eslint/js": "^8.57.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "22.19.2",
    "@types/react": "19.2.3",
    "@types/react-dom": "19.2.3",
    "@types/underscore.string": "^0.0.42",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "autoprefixer": "^10.4.22",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^14.2.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "11.0.0",
    "envalid": "^8.1.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-merge": "^2.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-nesting": "^13.0.2",
    "prettier": "^3.6.2",
    "swagger-typescript-api": "^13.0.23",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.5",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
